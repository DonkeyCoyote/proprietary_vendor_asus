on boot && property:ro.boot.product.vendor.sku=taro
    start spuservice

on boot && property:ro.boot.product.vendor.sku=cape
    start spuservice

service spuservice /vendor/bin/hw/vendor.qti.spu@2.0-service
    class hal
    user system
    group system
    disabled
